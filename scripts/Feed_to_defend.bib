issn = {1991-637X},
journal = {African Journal of Agricultural Research},
keywords = {bananas,chlorosis,effects,fertilizers,fruit growing,inoculation,latent infections,leaf area,leaves,mortality,necrosis,orchards,pathogens,plant height,plant pathogenic bacteria,plant pathogens,productivity,soil,symptoms,wilting},
number = {35},
pages = {2727--2735},
title = {{Interactive effects of fertilizer and inoculum concentration on subsequent development of Xanthomonas wilt in banana.}},
